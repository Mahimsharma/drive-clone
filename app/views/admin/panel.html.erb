<h1>Admin#create_user</h1>
<form  action="/admin/post_user" method="post">
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label for="name">First name:</label>
  <input type="text" id="name" name="name"><br><br>
  <label for="email">Email:</label>
  <input type="text" id="email" name="email"><br><br>
  <label for="number">Phone number:</label>
  <input type="number" id="number" name="number"><br><br>


  <input type="submit" value="Submit">
</form>
 <br><br>

 <% @users.each do |user| %>
 <%=  user.id %>
 <%=  user.name %>
 <%=  user.email %>
 <%=  user.number %>
 <br>
 <% end %>

 <br><br>

<h1>Admin#create_medium</h1>
<form  action="/admin/post_medium" method="post">
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label for="name">File name:</label>
  <input type="text" id="name" name="name"><br><br>
  <label for="owner_id">Owner id:</label>
  <input type="integer" id="owner_id" name="owner_id"><br><br>
  <label for="location">Path:</label>
  <input type="text" id="location" name="location"><br><br>

  <input type="submit" value="Submit">
</form>
 <br><br>
  <% @media.each do |medium| %>
 <%=  medium.name %>
 <%=  medium.owner_id %>
 <%=  medium.location %>
 <br>
 <% end %>

  <br><br>

<h1>Admin#Upload_file</h1>
<form  action="/admin/upload_file" method="post">
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label for="location">Path:</label>
  <input type="file" id="file" name="file"><br><br>
  <input type="submit" value="Submit">
</form>
 <br><br>
